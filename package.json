  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "mongoose": "^8.4.1",
    "tailwind-merge": "^2.3.0",
    "yup": "^1.4.0"
  }
