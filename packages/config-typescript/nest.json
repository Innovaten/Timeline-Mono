{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Nest",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "noEmit": false,
    "target": "ES2021",
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": false
  }
}
